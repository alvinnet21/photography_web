VITE_API_BASE_URL=http://localhost:8080

# If your API requires Bearer token auth (e.g., JWT)
# VITE_API_TOKEN=your_jwt_token_here

# If your API uses Basic Auth
# VITE_API_BASIC_USER=admin
# VITE_API_BASIC_PASS=secret

# If your API uses an API key header
# VITE_API_KEY=your_api_key
# VITE_API_KEY_HEADER=X-API-KEY

# Set to true if backend uses cookies/sessions and you need to send them
# VITE_API_WITH_CREDENTIALS=true

# Dev server proxy target (used in vite.config.ts). Set to your backend URL.
VITE_PROXY_TARGET=http://localhost:8080
